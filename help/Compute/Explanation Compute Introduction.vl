          <Pad Id="BOcllehG4znMvq6OUr4U0A" Comment="Element Count" Bounds="572,442,42,15" ShowValueBox="true" isIOBox="true" Value="1024">
            <p:TypeAnnotation LastCategoryFullName="Primitive" LastDependency="VL.CoreLib.vl">
